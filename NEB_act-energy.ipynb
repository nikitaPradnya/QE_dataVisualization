   "source": [
    "'''\n",
    "NEB PLOTTING: Activation energy (->)\n",
    "\n",
    "Notes:\n",
    "\n",
    "(I)\n",
    "Nilai x adalah nilai energi pada masing-masing tahap\n",
    "Array untuk nilai pada sumbu x: ([IS, IS, TS, FS, FS])\n",
    "\n",
    "IS: Initial state\n",
    "TS: Transition state\n",
    "FS: Final state\n",
    "\n",
    "(II)\n",
    "https://www.quantum-espresso.org/Doc/neb_user_guide.pdf\n",
    "'''\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import math as mat\n",
    "import numpy.polynomial.polynomial as poly"
   ]
  }, 
   
   "source": [
    "'''\n",
    "Surface: Clean\n",
    "Perfect ZnO (100) surface\n",
    "'''\n",
    "# Energy data\n",
    "x1 = np.array(range(0,2))\n",
    "y1 = np.zeros(2)\n",
    "x2 = np.array(range(1,4))\n",
    "y2 = np.array([0.0, 2.640434, 2.55227225])\n",
    "x3 = np.array(range(3,5))\n",
    "y3 = np.array([2.55227225, 2.55227225])\n",
    "\n",
    "print(x2)\n",
    "\n",
    "# Polynomial approach\n",
    "z = np.polyfit(x2, y2, 2)\n",
    "p = np.poly1d(z)\n",
    "\n",
    "# Plotting\n",
    "xp = np.linspace(1, 3)\n",
    "plt.plot(x1, y1, 'b-', xp, p(xp), 'b-', x3, y3, 'b-')\n",
    "plt.xticks([])\n",
    "plt.xlabel('States')\n",
    "plt.ylabel('Energy (eV)')\n",
    "plt.title('Perfect ZnO (100) Surface (Clean)')\n",
    "plt.grid(which='both', linestyle='--')\n",
    "plt.show()\n"
   ]
  },
